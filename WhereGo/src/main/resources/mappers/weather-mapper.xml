<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="weatherMapper">
	<select id="selectTcode" parameterType="string" resultType="string">
		SELECT CODE_TEMPERATURE FROM WEATHER
		WHERE LOCATION_NAME=#{location}
	</select>
	<select id="selectWcode" parameterType="string" resultType="string">
		SELECT CODE_WEATHER FROM WEATHER
		WHERE LOCATION_NAME=#{location}
	</select>
</mapper>